  "private": false,
  "scripts": {
    "dist": "webpack --progress --colors",
    "fetch": "ts-node src/index.ts fetch",
    "clean": "ts-node src/index.ts clean",
    "build": "ts-node src/index.ts build",
    "sync": "ts-node src/index.ts sync",
    "pull": "ts-node src/index.ts sync pull",
    "push": "ts-node src/index.ts sync push",
    "purge": "ts-node src/index.ts sync purge",
    "build:all": "ts-node src/index.ts clean && ts-node src/index.ts fetch && ts-node src/index.ts",
    "dev": "ts-node-dev --respawn src/index.ts build",
    "up": "cd tmp/ArknightsGameData && git pull"
